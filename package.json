    "body-parser": "^1.19.0",
    "concurrently": "^4.1.0",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "mongoose": "^5.5.2",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "stripe": "^6.31.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.11"
